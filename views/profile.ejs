      <script>
        document.addEventListener("DOMContentLoaded", function() {
    typeWriterEffect("typewriter1", "Welcome back, <%=user.fullName%>! We're excited to have you here.", 100);
    setTimeout(function() {
        typeWriterEffect("typewriter2", "Feel free to explore our features and connect with others.", 100);
    }, 5000); // Delay second line by 5 seconds to allow the first one to finish
});

function typeWriterEffect(elementId, text, speed) {
    let i = 0;
    function typeWriter() {
        if (i < text.length) {
            document.getElementById(elementId).innerHTML += text.charAt(i);
            i++;
            setTimeout(typeWriter, speed);
        }
    }
    typeWriter();
}

      </script>
