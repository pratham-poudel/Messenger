<style>
    .typewriter {
    border-right: 2px solid #333; /* The cursor */
    white-space: nowrap;
    font-family: monospace;
    overflow: hidden;
    display: inline-block;
    animation: blink-caret 0.75s step-end infinite;
}

@keyframes blink-caret {
    from, to { border-color: transparent; }
    50% { border-color: #333; }
}

</style>
